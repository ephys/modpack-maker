{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "concurrently npm:start:dev:app npm:start:dev:db",
    "start:dev:db": "docker-compose up",
    "start:dev:app": "NODE_ENV=development nest start --watch --watchAssets",
    "start:debug": "nest start --debug --watch --watchAssets",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ephys/sequelize-cursor-pagination": "^1.1.0",
    "@iarna/toml": "^2.2.5",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/graphql": "^9.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/schedule": "^1.0.1",
    "apollo-server-express": "^3.4.0",
    "bull": "^3.29.3",
    "cheerio": "^1.0.0-rc.5",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cls-hooked": "^4.2.2",
    "core-js": "^3.18.3",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql": "^15.6.1",
    "graphql-tools": "^8.2.0",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "lucene": "^2.1.1",
    "nanoid": "^3.1.30",
    "node-fetch": "^2.6.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "semver": "^7.3.5",
    "sequelize": "^6.8.0",
    "sequelize-typescript": "^2.1.1"
  },
  "devDependencies": {
    "@foobarhq/eslint-config": "^11.1.1",
    "@foobarhq/eslint-config-typescript": "^11.1.1",
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/bull": "^3.15.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/lucene": "^2.1.3",
    "@types/node": "^16.11.3",
    "@types/node-fetch": "^2.5.12",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
